{
    "name": "secu4everyone",
    "description": "secu4everyone",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "128": "icon_128.png"
      },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "./popup-sign-in.html"
    },
    "permissions": [
        "identity"
    ] 
}