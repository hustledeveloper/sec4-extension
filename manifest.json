{
  "name": "sec4everyone",
  "description": "sec4everyone",
  "version": "1.0.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "background.js"
  },


  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },

  "permissions": ["activeTab"],

  "host_permissions": [
    "http://www.securityforeveryone.com/"
    
  ],

  "action": {
    "default_icon": "icon_16.png",
    "default_popup": "popup.html"
  }
}
