{
    "name": "secu4everyone",
    "description": "secu4everyone",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "128": "icon_128.png"
      },
      "permissions": ["tabs", "scripting", "identity", "identity","storage" ],
    "host_permissions": [
        "https://core-test.s4e.link/"
 
      ],
      "optional_host_permissions": [
        "*://*/*"
      ],
    "background": {
		"service_worker": "background.js"
	},
    "options_page": "./options.html",
    "action": {
		"default_popup": "./popup-sign-out.html"
	},



    "content_scripts": [
		{			
            "matches": [
				"<all_urls>"
            ],
            
			"js": [
				"content-script.js"
			]
		}
	]
}



	
